issn = {0035-8711},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Cosmic rays,Instabilities,MHD,Scattering,Turbulence},
mendeley-groups = {CR related},
month = {jun},
number = {3},
pages = {1195--1202},
primaryClass = {astro-ph},
publisher = {Oxford University Press},
title = {{Cosmic Ray Scattering in Compressible Turbulence}},
url = {https://academic.oup.com/mnras/article-lookup/doi/10.1111/j.1365-2966.2006.11093.x http://arxiv.org/abs/astro-ph/0606737 http://dx.doi.org/10.1111/j.1365-2966.2006.11093.x},
volume = {373},
year = {2006}
