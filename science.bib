@article{Mertsch:2013gg,
author = {Mertsch, Philipp and Sarkar, Subir},
title = {{Loops and spurs: the angular power spectrum of the Galactic synchrotron background}},
journal = {J. Cosmol. Astropart. Phys.},
year = {2013},
volume = {2013},
number = {06},
pages = {041--041},
month = jun,
