title = {{An improved map of the Galactic Faraday sky}},
journal = {A\&A},
year = {2012},
volume = {542},
pages = {93},
month = jun,
affiliation = {Max Planck Institute for Astrophysics, Karl-Schwarzschild-Str. 1, 85741, Garching, Germany niels@mpa-garching.mpg.de},
